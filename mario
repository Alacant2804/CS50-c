#include <cs50.h>
#include <stdio.h>

int main(void)
{
    int height;
    do
    {
        height = get_int("Height: ");
    }
    while(height < 1 || height > 8)
    // left side

    for (int row = 0; row < height; row++)
    {
        for (int col = 0; col <= row: col++)
        {
            printf("#\n");
        }
    }
char *s = ("What's your name? ");

for (int i = 0; i < height; i++)
{
    printf("?");

}
}